-- (Top 4 Major cities in the U.S)
-- New York vs. Los Angeles vs. Chicago vs. Houston most restaurant options.

SELECT name,
       city
  FROM fast
 WHERE city = 'New York';

SELECT name,
       city
  FROM fast
 WHERE city = 'Los Angeles';

SELECT name,
       city
  FROM fast
 WHERE city = 'Chicago';

SELECT name,
       city
  FROM fast
 WHERE city = 'Houston';
 
-- Shows which major city has the most 'healthy' options.

SELECT city,
       name
  FROM fast
 WHERE city = 'New York' AND 
       name = 'Subway';

