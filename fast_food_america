SELECT *
  FROM fast;
  
SELECT name, latitude, longitude
  FROM fast;
  
-- Shows states with the most and least fast food restaurants.

SELECT province,
       COUNT(province) AS total_province
  FROM fast
 GROUP BY province
 ORDER BY total_province DESC;
 
-- Shows cities with the most and least fast food restaurants.

SELECT city,
       COUNT(city) AS total_city
  FROM fast
 GROUP BY city
 ORDER BY total_city DESC;
 
-- Shows restaurants with the most locations nationally.

SELECT name,
       COUNT(name) AS total_nationally
  FROM fast
 GROUP BY name
 ORDER BY total_nationally DESC;
 
-- Shows corrections with spelling throughout the table in order to have accurate numbers.

